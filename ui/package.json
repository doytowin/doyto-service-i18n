{
  "name": "doyto-service-i18n-ui",
  "version": "1.1.0",
  "description": "多语言管理系统",
  "author": "袁臻 <f0rb@163.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "build": "^0.1.4",
    "core-js": "^2.6.11",
    "element-ui": "^2.13.1",
    "run": "^1.4.0",
    "sass": "^1.56.1",
    "vue": "^2.6.11",
    "vue-i18n": "^7.4.0",
    "vue-resource": "^1.5.3",
    "vue-router": "^3.1.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-e2e-nightwatch": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
