    "globals": {
      "axios": "writable",
      "Util": "writable",
      "Cons": "writable",
      "Crud": "writable"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
